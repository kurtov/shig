<% content_for :left_nav do%>
  <%= render :partial => 'shared/navbar', :locals => { :game => @game } %>
<% end %>

<% content_for :page_name do %>
  Редактировать игру
<% end %>

<%= render :partial => 'shared/error_messages', :locals => { :target => @game } %>
<%= form_for @game, :url => game_path(@game), :method => :put, :html => {:class => "form-horizontal", :role => "form"} do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>

  <div class="form-group">
    <div class="col-lg-offset-2 col-lg-10">
      <%= f.submit "Изменить", :class => "btn btn-default"%>
    </div>
  </div>
<% end %>